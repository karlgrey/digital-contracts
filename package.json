{
  "name": "stellplatz-booking",
  "version": "1.0.0",
  "description": "Digitale Stellplatz-Vermietung mit automatischer Vertragserstellung",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "init-db": "node -e \"require('./database.js')\"",
    "seed": "node seed.js",
    "reset-db": "rm -f stellplatz.db && npm run init-db && npm run seed"
  },
  "dependencies": {
    "better-sqlite3": "^11.8.1",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "pdfkit": "^0.13.0",
    "resend": "^6.9.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
